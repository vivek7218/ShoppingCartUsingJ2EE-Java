<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
 
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.shoppingCart.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.shoppingCart.servlet.LogoutServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>AddToCartServlet</servlet-name>
  	<display-name>AddToCartServlet</display-name>
  	<description></description>
  	<servlet-class>com.shoppingCart.servlet.AddToCartServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>quantityIncrDecreServlet</servlet-name>
  	<display-name>quantityIncrDecreServlet</display-name>
  	<description></description>
  	<servlet-class>com.shoppingCart.servlet.quantityIncrDecreServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>QuantityIncrDecrServlet</servlet-name>
  	<display-name>QuantityIncrDecrServlet</display-name>
  	<description></description>
  	<servlet-class>com.shoppingCart.servlet.QuantityIncrDecrServlet</servlet-class>
  </servlet>
