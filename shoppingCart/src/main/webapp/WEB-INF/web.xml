  	<display-name>OrderNowServlet</display-name>
  	<description></description>
  	<servlet-class>com.shoppingCart.servlet.OrderNowServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>CheckOutServlet</servlet-name>
  	<display-name>CheckOutServlet</display-name>
  	<description></description>
  	<servlet-class>com.shoppingCart.servlet.CheckOutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CancelOrderServlet</servlet-name>
  	<display-name>CancelOrderServlet</display-name>
  	<description></description>
  	<servlet-class>com.shoppingCart.servlet.CancelOrderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/user-login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/log-out</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddToCartServlet</servlet-name>
  	<url-pattern>/add-to-cart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>quantityIncrDecreServlet</servlet-name>
